<Alloy>
    <Window>
        <TableView>
            <TableViewRow>
                <Label class="left" id="id" />
            </TableViewRow>
            <TableViewRow>
                <Label class="left" id="title" />
            </TableViewRow>
            <TableViewRow>
                <Label class="left" id="description" />
            </TableViewRow>
            <TableViewRow>
                <Label class="left" id="address" />
            </TableViewRow>
            <TableViewRow onClick="imageClick">
                <ImageView class="left" id="image" />
            </TableViewRow>
            <TableViewRow>
                <Label class="left" id="panorama" onClick="panoramaClick" />
            </TableViewRow>
            <TableViewRow>
                <Label class="left" id="latitude" />
            </TableViewRow>
            <TableViewRow>
                <Label class="left" id="longitude" />
            </TableViewRow>
        </TableView>
    </Window>
</Alloy>